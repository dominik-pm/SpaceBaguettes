onready var lab_message = $LabWinnerMessage
onready var anim = $AnimationPlayer
onready var rocket = $Elements/Rocket
onready var player_sprite = $Elements/Rocket/PlayerSprite
onready var statistics = $Statistics

onready var btn_play = $CenterContainer/VBoxContainer/BtnPlay

func _ready():
	if Network.local_id != null and Network.local_id != 1:
		btn_play.hide()

func show_summary(winner_pid, stats):
	show()
	$CenterContainer/VBoxContainer/BtnPlay.grab_focus()
	var n = Global.player_names[int(winner_pid)-1]
	lab_message.set_winner(n)
	anim.play(\"show\")
	rocket.fly_in()
	player_sprite.init(int(winner_pid))
	
	statistics.show_player_stats(stats)
"

